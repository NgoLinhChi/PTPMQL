@model FirstWebMVC.Models.Employee

@{
    ViewData["Title"]= "Create";
}
<h1>Create</h1>
<hr>
<div class ="row">
    <div class="col-md-4">
        <form asp-action="Create" class="w3-container w3-card-4">
              <h2 class="w3-text-grey">Employee</h2>
            <div asp-validation-summary="ModelOnly" class="text-danger "></div>
            <div class=form-group>
                <label asp-for="EmployeeID" class="control-label w3-text-grey"></label>
                <input asp-for="EmployeeID" class="form-control w3-input w3-border">
                <span asp-validation-for="EmployeeID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Age" class="control-label w3-text-grey"></label>
                <input asp-for="Age" class="form-control w3-input w3-border">
                <span asp-validation-for="Age" class="text-danger"></span>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary w3-input w3-border w3-grey">
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}